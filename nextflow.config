params {
  
  outdir = "result"
  data_preprocess = 'False'
  call_variant = 'False'
  filter_variant = 'True'
  demultiplex = 'False'

  demux_tool  = "demuxlet"
  demuxlet_outdir = "$projectDir/result"

  sam = "$projectDir/data/jurkat_293t_downsampled_n500_full_bam.bam"
  tag_group = 'False'
  tag_UMI = 'False'  
  plp = 'no_plp'
  vcf = "$projectDir/data/jurkat_293t_exons_only.vcf"
  field = "GT"
  geno_error_offset = 0.1
  geno_error_coeff = 0.0
  r2_info = 'R2'
  min_mac = 1
  min_callrate = 0.50 
  sm = 'False'
  sm_list = 'no_sm_list'
  alpha = 0.5
  alpha_list = 'False'
  doublet_prior = 0.50
  sam_verbose = 1000000
  vcf_verbose = 10000
  cap_BQ = 40
  min_BQ = 13
  min_MQ = 20
  min_TD = 0
  excl_flag = 3844
  group_list = 'False'
  min_total = 0   
  min_umi = 0
  min_snp = 0


  celldata = "$projectDir/data/cells.cellSNP.vcf.gz"
  vartrixData = 'no_vartrixData'
  donorfile = 'no_donorfile'
  genoTag = 'PL'
  ndonor = 4
  noDoublet = 'False'
  nInit = 50
  extraDonor = 0  
  extraDonorMode = 'distance'
  forceLearnGT = 'False'
  ASEmode = 'False'
  noPlot = 'False'
  randSeed = 'none'
  cellRange = 'all'
  callAmbientRNAs = 'False'
  nproc = 1


  bam = "/souporcell/jurkat_293t_downsampled_n500_full_bam.bam"
  barcodes = "/souporcell/barcodes.tsv"
  fasta = "/souporcell/refdata-cellranger-hg19-3.0.0/fasta/genome.fa"
 
  threads = 2

  ploidy = 2
  min_alt = 10
  min_ref = 10
  max_loci = 2048
  restarts = 'False'
  common_variants = 'no_common_variants'
  known_genotypes = 'no_known_genotypes'
  known_genotypes_sample_names = 'False'
  skip_remap = 'False'
  ignore = 'False'
   

  variant_tool = "freebayes"
  bam_freebayes = "$projectDir/data/jurkat_293t_downsampled_n500_full_bam.bam"
  stdin = 'False'
  fasta_reference = "$projectDir/souporcell/refdata-cellranger-hg19-3.0.0/fasta/genome.fa"
  targets = 'no_targets'
  region = 'False'
  samples= 'no_samples'
  cnv_map = 'no_cnv_map'
  vcf_freebayes = "vcf_output.vcf"
  gvcf = 'False'
  gvcf_chunk = 'False'
  gvcf_dont_use_chunk = 'false'
  variant_input = 'no_variant_input'
  only_use_input_alleles = 'False'
  haplotype_basis_alleles = 'no_haplotype_basis_alleles'
  report_all_haplotype_alleles = 'False'
  report_monomorphic  = 'False'
  pvar = 0.0
  strict_vcf  = 'False'
  theta = 0.001
  pooled_discrete = 'False'
  pooled_continuous = 'False'
  use_reference_allele = 'False'
  reference_quality = '100,60'
  use_best_n_alleles = 0
  haplotype_length = 3
  min_repeat_size = 5
  min_repeat_entropy = 1
  no_partial_observations = 'False'
  throw_away_snp_obs = 'False'
  throw_away_indel_obs = 'True'
  throw_away_mnps_obs = 'True'
  throw_away_complex_obs = 'True'
  dont_left_align_indels = 'false'
  use_duplicate_reads = 'false'
  min_mapping_quality = 1
  min_base_quality = 0
  min_supporting_allele_qsum = 0
  min_supporting_mapping_qsum = 0
  mismatch_base_quality_threshold = 10
  read_mismatch_limit = 'False'
  read_max_mismatch_fraction = 1.0
  read_snp_limit = 'False'
  read_indel_limit = 'False'
  standard_filters = 'False'
  min_alternate_fraction = 0.05
  min_alternate_count = 2
  min_alternate_qsum = 0
  min_alternate_total = 1
  min_coverage = 0
  limit_coverage = 'False'
  skip_coverage = 'False'    
  no_population_priors = 'False'
  hwe_priors_off = 'False'
  binomial_obs_priors_off = 'False'
  allele_balance_priors_off = 'False'
  observation_bias = 'no_observation_bias'
  base_quality_cap = 'False'
  prob_contamination = 0.000000001
  legacy_gls= 'False'
  contamination_estimates = 'no_contamination_estimates'
  report_genotype_likelihood_max = 'False'
  genotyping_max_iterations = 1000
  genotyping_max_banddepth = 6
  posterior_integration_limits = '1,3'
  exclude_unobserved_genotypes = 'False'
  genotype_variant_threshold = 'False'
  use_mapping_quality = 'False'
  harmonic_indel_quality = 'False'
  read_dependence_factor = 0.9
  genotype_qualities= 'False'
  debug = 'False'
  dd = 'False'
  bam_list = 'no_bam_list'
  populations = 'no_populations'

  
  vcfscSplit = "$projectDir/scSplit/filtered.vcf"
  bamscSplit = "$projectDir/scSplit/sorted.bam"
  barscSplit = "$projectDir/scSplit/barcodes.tsv"
  tagscSplit = 'CB'
  comscSplit = "$projectDir/scSplit/common_SNP_hg19.vcf"
  refscSplit = 'ref_filtered.csv'
  altscSplit = 'alt_filtered.csv'
  numscSplit = 2
  subscSplit = 10
  emsscSplit = 30
  dblscSplit = 'False'
  vcf_known_scSplit = "$projectDir/scSplit/jurkat_293t_exons_only.vcf"
  scSplit_loc = "$projectDir/scSplit"
  sample_geno = 'False'



  samFile = "$projectDir/data/jurkat_293t_downsampled_n500_full_bam.bam"
  samFileList = 'no_samFileList'
  regionsVCF  = "$projectDir/data/genome1K.phase3.SNP_AF5e2.chr1toX.hg19.vcf.gz"
  targetsVCF = 'no_targetsVCF'
  barcodeFile = "$projectDir/scSplit/barcodes.tsv"
  sampleList  = 'no_sampleList'
  sampleIDs = 'no_sampleIDs'
  genotype_cellSNP = 'False'
  gzip_cellSNP = 'True'
  printSkipSNPs = 'False'
  nproc_cellSNP = 5
  refseq_cellSNP = 'no_refseq'
  chrom = 'False'
  cellTAG = 'CB'
  UMItag = 'Auto'
  minCOUNT = 20
  minMAF = 0.1
  doubletGL = 'False'
  inclFLAG = 'False'
  exclFLAG = 'False'
  minLEN = 30
  minMAPQ = 20
  maxDEPTH = 0
  countORPHAN = 'False'
  outDir_cellSNP = '.'

}

process {
    withName:demuxlet{
        container = 'popscle'
    }
    withName:vireo {
        container = 'vireo'
    }
    withName:freebayes {
        container = 'freebayes'
    }
    withName:scSplit {
        container = 'scsplit'
    }
    withName:cellSNP {
        container = 'quay.io/biocontainers/cellsnp-lite:1.2.2--h6233b05_2'
    }
    withName:samstool {
        container = 'biocontainers/samtools:v1.9-4-deb_cv1'
    }
    withName:bcftools {
        container = 'biocontainers/bcftools:v1.9-1-deb_cv1'
    }
    
}
docker.enabled = true

